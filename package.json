{
  "name": "ask-insight-client",
  "version": "1.0.0",
  "description": "Client library for ASK Insights",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf coverage dist doc",
    "compile": "tsc -p tsconfig.json",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "nyc -x tst -e .ts --temp-directory 'coverage/nyc-output' -r html -r text-summary -r cobertura _mocha --require ts-node/register 'tst/**/*.spec.ts' --reporter nyan",
    "doc": "typedoc --excludeExternals --mode file --out doc lib/",
    "build": "npm-run-all clean compile lint test doc",
    "prerelease": "npm run build",
    "release": "git add -A && standard-version -a"
  },
  "standard-version": {
    "scripts": {
      "postcommit": "git push && git push --tags"
    }
  },
  "keywords": [
    "Alexa",
    "ASK",
    "Skill",
    "Logging"
  ],
  "author": "Taimos GmbH",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/taimos/ask-insight-client.git"
  },
  "peerDependencies": {
    "ask-sdk-model": "^1.11.2",
    "ask-sdk-core": "^2.3.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "@types/sinon": "^4.3.0",
    "chai": "^4.1.2",
    "mocha": "^5.0.5",
    "nock": "^9.2.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "standard-version": "^4.4.0",
    "ts-node": "^6.0.1",
    "tslint": "^5.9.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.1"
  },
  "dependencies": {
    "ask-sdk-core": "^2.3.0",
    "ask-sdk-model": "^1.11.2"
  }
}
